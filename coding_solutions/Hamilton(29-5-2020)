
def printElements(arr, n): 

      # Traverse array from index 1 to n-2 

    # and check for the given condition 

    for i in range(1, n-1, 1): 

        if (arr[i] > arr[i - 1] and 

            arr[i] > arr[i + 1]): 

            print(arr[i], end = " ") 

  


if __name__ == '__main__': 
	arr = [ ] 
	n=int(input())
	for i in range(0,n):
		ele=int(input())
		arr.append(ele)
	printElements(arr, n) 
